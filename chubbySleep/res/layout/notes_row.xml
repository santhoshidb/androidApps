<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
      android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    

    
<TableLayout
        android:layout_height="wrap_content"
        android:id="@+id/TableLayout002"
        android:layout_width="wrap_content" android:paddingTop="20dp">
<TableRow>
<TextView android:id="@+id/cdateh"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:text="Created"
    android:layout_height="wrap_content" android:padding="10dp" android:textColor="#000000" android:textStyle="bold"/>
    
<TextView android:id="@+id/bdateh"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:text="Birthday"
    android:layout_height="wrap_content" android:padding="10dp" android:textColor="#000000" android:textStyle="bold"/>

<TextView android:id="@+id/percenth"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:text="Percentile"
    android:layout_height="wrap_content" android:padding="10dp" android:paddingLeft="4dp" android:textColor="#000000" android:textStyle="bold"/>
</TableRow>
<TableRow>

<TextView android:id="@+id/cdate"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" android:padding="10dp" android:textColor="#000000"/>
    
<TextView android:id="@+id/bdate"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" android:padding="10dp" android:textColor="#000000"/>

<TextView android:id="@+id/percent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" android:padding="10dp" android:paddingLeft="4dp" android:textColor="#000000"/>
</TableRow>
</TableLayout>

</RelativeLayout>

